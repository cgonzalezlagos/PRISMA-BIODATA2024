@ARTICLE{Villaseñor2024,
	author = {Villaseñor, Nélida R. and Muñoz-Pacheco, Catalina B. and Escobar, Martín A. H.},
	title = {Opposite Responses of Native and Nonnative Birds to Socioeconomics in a Latin American City},
	year = {2024},
