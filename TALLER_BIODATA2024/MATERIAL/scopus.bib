	affiliations = {Departamento de Ecología, Pontificia Universidad Católica de Chile, Santiago, Alameda 340, Chile; Institute of Ecology and Biodiversity (IEB), Santiago, Casilla 653, Chile; Departamento de Ciencias Ecológicas, Universidad de Chile, Santiago, Casilla 653, Chile},
