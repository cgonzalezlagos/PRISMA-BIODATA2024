	language = {English},
	abbrev_source_title = {The Vicuña: The Theor. and Pract. of Comm. Based Wildl. Mgmt.},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 2}
}

