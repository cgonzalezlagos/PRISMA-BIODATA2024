@ARTICLE{Li2016211,
	author = {Li, M. and Zhang, Y.Y. and Wang, K. and Hou, Y.G. and Zhou, H.Y. and Jin, L. and Zhao, J. and Chen, W.D.},
	title = {First report of sunflower white mold caused by Sclerotinia minor jagger in Inner Mongolia region, China},
	year = {2016},
	journal = {Plant Disease},
	volume = {100},
	number = {1},
	pages = {211},
	doi = {10.1094/PDIS-01-15-0027-PDN},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84961377605&doi=10.1094%2fPDIS-01-15-0027-PDN&partnerID=40&md5=4a4e5dd607bc9b47d68b3fcb34733c54},
	affiliations = {College of Agronomy, Inner Mongolia Agricultural University, Hohhot, China; USDA ARS, Washington State University, Pullman, 99164, WA, United States},
	abstract = {During the 2014 growing season, 20 to 30 cm brown stringy rot at basal stems and wilt of plants was found at R5.9-R6 stage of sunflower (Helianthus annulus) variety LD 5009 in Chayouzhongqi, Siziwangqi, Xinghe, and Huade areas of Wulanchabu City, Inner Mongolia, China. The average disease incidence was between 10 and 15%. Tiny (0.30 to 2.68 mm), black, round sclerotia were observed on the infected stems. Although sunflower white mold caused by Sclerotinia sclerotiorum has been observed in this region before, the sclerotia observed on the diseased plants were smaller than those produced by S. sclerotiorum. From a total of 171 samples, seven were chosen randomly for pathogen identification. Tiny sclerotia collected from each infected stem were surface-sterilized in 75% alcohol for 30 s, and in 0.5% NaClO for 3 min, then placed on potato dextrose agar (PDA) and incubated for 72 h at 20 to 24°C without light. The emerging mycelia were hyphal-tipped three times and subcultured on PDA. The sclerotial morphology resembled those of S. minor Jagger (Kohn 1979). To confirm the species molecularly, total genomic DNA was isolated from mycelia. The internal transcribed spacer of nuclear rDNA was amplified with PCR using the primer pair ITS1 (5′-TCCGTAGGTGAACCTGCGG-3′) and ITS4 (5′-TCCTCCGCTTATTGATATGC-3′) (White et al. 1990). The ITS sequences of all seven isolates were the same and 99% identical to those of S. minorsequence deposited in the GenBank database (KF859929.1). One ITS sequence from this study was deposited in GenBank and assigned accession number KP340982. Pathogenicity was tested by using seven purified isolates as follow: 8-week-old sunflower plants (two plants per 7-cm diameter pot; 10 plants per isolate) were inoculated using the stem inoculation method. Stem inoculation was performed by wounding the stem near soil line using a sterilized toothpick, then 9-mm diameter PDA plugs with actively growing mycelium were placed on the wound sites. A plain PDA plug without mycelium was used as a control. Inoculation sites on all tested plants were wrapped with plastic to maintain moisture, and all the inoculation plants were kept under a 16-h photoperiod with temperatures of 20 to 25°C and 40% relative humidity. Water-soaked lesions were observed at the inoculation sites of all plants 2 days post inoculation (dpi). The lesion expanded along the stem, and white mycelium and also black tiny sclerotia were observed at the infection sites 5 dpi. At 7 dpi, plants withered and lodged. No symptoms were observed on the control plants. The fungus was consistently reisolated from the inoculated stem and tiny sclerotia formed on PDA plates were consistent with S. minor. Although S. minor causing sunflower white mold has been reported in Australia, Bulgaria, Canada, Chile, Kenya, New Zealand, Spain, and the United States (Farr and Rossman 2014), to the best of our knowledge, this is the first report of S. minor-caused sunflower white mold in China. Because potato is the main rotated crop with sunflower in these regions, it is believed that the S. minor on sunflower was from infected potato. This finding is important to guide the local farmers to choose the right rotated crop and to provide a new research topic for local plant pathologists. © 2016, American Phytopathological Society. All rights reserved.},
	publisher = {American Phytopathological Society},
	issn = {01912917},
	coden = {PLDID},
	language = {English},
	abbrev_source_title = {Plant Dis.},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 4; All Open Access, Hybrid Gold Open Access}
}

